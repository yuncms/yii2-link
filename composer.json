{
  "name": "yuncms/yii2-link",
  "description": "The tag link for the yii framework",
  "keywords": [
    "yuncms",
    "link"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "yuncms",
      "email": "yuncms@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "xutl/yii2-fileupload-widget": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\link\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "link",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/link/message"
      },
      "migrationNamespace": "yuncms\\link\\migrations",
      "backend": {
        "class": "yuncms\\link\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\link\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
